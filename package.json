    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-config": "^0.0.2",
    "node-gyp": "^8.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "prisma": "^2.24.1",
